                                    <![CDATA[from subprocess import call

class MyClass(GeneratedClass):
    def __init__(self):
        GeneratedClass.__init__(self)
        self.memory = ALProxy("ALMemory")

    def onLoad(self):
        #put initialization code here
        pass

    def onUnload(self):
        #put clean-up code here
        pass

    def onInput_onStart(self):

        self.memory.insertData("pepperAbanca/imagen", "")
        import requests
        import json
        import urllib

        headers = {"Content-Type": "application/json","Accept": "application/json"}
        data = {"method":"actTakePicture", "params":[], "id":1, "version":"1.0"}

        try:
            resp = requests.post('http://10.0.0.1:10000/sony/camera', headers=headers, json=data, allow_redirects=False)

            rJson = resp.json()
            self.memory.insertData("pepperAbanca/imagenJson", str(rJson))
            #self.memory.insertData("pepperAbanca/imagen", str(rJson['result'][0]))

            urlImage = str(rJson['result'][0])
            urlImage2 = urlImage.split("'")
            self.memory.insertData("pepperAbanca/imagen", urlImage2[1])

            urllib.urlretrieve(urlImage2[1], "/var/www/apps/"+self.packageUid()+"/html/images/local-image.jpg")

            #self.memory.insertData("pepperAbanca/imagen", resp.json())
            self.memory.raiseEvent("Abanca/pepperSelfie/fotoHecha", 1)
            self.onStopped()

        except requests.exceptions.Timeout:
            self.logger.info("Error Timeout")
            # Maybe set up for a retry, or continue in a retry loop
        except requests.exceptions.TooManyRedirects:
            self.logger.info("Error too many")
        except requests.exceptions.RequestException as e:
            self.logger.info("Algo no ha ido bien: "+ str(e))
            self.memory.raiseEvent("Abanca/pepperSelfie/fotoNoHecha", 1)
            self.onStopped()
        pass

    def onInput_onStop(self):
        self.onUnload() #it is recommended to reuse the clean-up as the box is stopped
        self.onStopped() #activate the output of the box]]>
