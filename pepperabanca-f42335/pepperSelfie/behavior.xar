                                <content>
                                    <![CDATA[class MyClass(GeneratedClass):
    def __init__(self):
        GeneratedClass.__init__(self)

    def onLoad(self):
        self.connection = ALProxy("ALConnectionManager")
        #put initialization code here
        pass

    def onUnload(self):
        #put clean-up code here
        pass

    def onInput_onStart(self):
        ##### Desconectamos pepper de la cámara y lo conectamos a la red
        try:
            self.idWifiCamara = ""
            self.camaraDisponible = False
            self.connectServices()

            if not self.camaraDisponible:
                self.logger.info("No he podido conectarme")
                self.connection.scan()
                self.connectServices()

            if not self.camaraDisponible:
                self.logger.info("No he podido conectarme")
                self.cam_not_connected()
            else:
                if(self.isConnected()):
                    self.logger.info("Camara conectada")
                    self.cam_connected()
                else:
                    self.connection.connect(self.idWifiCamara)
                    if(self.isConnected()):
                        self.logger.info("Camara conectada")
                        self.cam_connected()
                    else:
                        self.logger.info("No ha sido posible conectarse a la cámara")
                        self.cam_not_connected()
            #self.connected() #activate the output of the box
        except OSError as err:
            self.logger.info("Algo no ha ido bien")
            self.cam_not_connected()
        pass

    def onInput_onStop(self):
        self.onUnload() #it is recommended to reuse the clean-up as the box is stopped
        self.cam_not_connected() #activate the output of the box

    def connectServices(self):
        servicios = self.connection.services()
        for serv in servicios:
            network = dict(serv)
            #if (network["Name"] == "TheRobotMuseum"):
            if (network["Name"] == "DIRECT-mhQ0:DSC-QX10"):
                self.camaraDisponible = True
                self.idWifiCamara = network["ServiceId"]

    def isConnected(self):
        propertiesService = self.connection.service(self.idWifiCamara)
        propertiesService = dict(propertiesService)
        if (propertiesService["State"] == "idle"):
            return False
        else:
            return True]]>
